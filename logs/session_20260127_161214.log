def get_ice_stats(green_url, swir_url, bbox):
    try:
        with rasterio.open(green_url) as src:
            left, bottom, right, top = transform_bounds('EPSG:4326', src.crs, *bbox)
            window = from_bounds(left, bottom, right, top, src.transform)
